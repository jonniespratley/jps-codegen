					<mx:TextInput id="txt_type" text="{ model.SELECTED_PROJECTS.type }" width="100%"/>
				</mx:FormItem>		</mx:Form>		<!-- Projects Action Buttons -->		<mx:ControlBar>						<mx:Button				label="Save"				click="validateAndSubmit()"/>						<mx:Button				label="Clear"				click="clear()"/>		</mx:ControlBar>	</mx:Panel>	<!--All the validators for required inputs-->	
