/**This is a sample application.*/package com.test.app.business{	import com.adobe.cairngorm.business.ServiceLocator;		import mx.rpc.AsyncToken;	import mx.rpc.IResponder;	import com.test.app.vo.*;	public class UploadsServiceDelegate	{		private var responder:IResponder;		private var service:Object;		public function UploadsServiceDelegate( responder:IResponder )		{			this.service = ServiceLocator.getInstance().getHTTPService( "UploadsService" );			this.responder = responder;		}				public function getAllUploads():void		{			var token:AsyncToken = service.send( {												m: 'get'												} );				 token.addResponder( responder );		}						public function saveUploads( vo:UploadsVO ):void		{			var token:AsyncToken = service.send( {												m: 'save',																								} );				 token.addResponder( responder );		}					public function removeUploads( vo:UploadsVO, index:int ):void		{			var token:AsyncToken = service.send( {												m: 'remove',																								} );				token.addResponder( responder );				token.vo = vo;				token.index = index;		}			}}                     