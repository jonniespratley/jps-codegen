<?xml version="1.0"?>
<flex-config>
	<compiler>


		<!-- List of path elements that form the roots of ActionScript class hierarchies. -->

      <source-path>
         <path-element>.</path-element>
      </source-path>

		<debug>true</debug>
		<include-libraries>
			<library>libs/Cairngorm.swc</library>
			<library>libs/as3json.swc</library>
		</include-libraries>
	</compiler>
	

		
		

